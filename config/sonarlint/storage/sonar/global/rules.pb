<pre>
if (condition)
{
  FirstActionInBlock();
  SecondAction();
}
ThirdAction();

string str = null;
for (int i = 0; i &lt; array.Length; i++)
{
  str = array[i];
  DoTheThing(str);
}
</pre>
<h2>See</h2>
